{
  "name": "@cc-task-manager/schemas",
  "version": "1.0.0",
  "description": "Shared validation schemas for CC Task Manager",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js",
      "require": "./dist/auth/index.js",
      "default": "./dist/auth/index.js"
    },
    "./tasks": {
      "types": "./dist/tasks/index.d.ts",
      "import": "./dist/tasks/index.js",
      "require": "./dist/tasks/index.js",
      "default": "./dist/tasks/index.js"
    },
    "./settings": {
      "types": "./dist/settings/index.d.ts",
      "import": "./dist/settings/index.js",
      "require": "./dist/settings/index.js",
      "default": "./dist/settings/index.js"
    },
    "./analytics": {
      "types": "./dist/analytics/index.d.ts",
      "import": "./dist/analytics/index.js",
      "require": "./dist/analytics/index.js",
      "default": "./dist/analytics/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "zod": "^3.22.0",
    "@nestjs/config": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "^5.1.3"
  }
}